{
  "version": "1.0.0-alpha-*",
  "description": ".Net wrapper for CRoaring - a C implementation of Roaring Bitmaps",
  "authors": [ "RogueException" ],

  "packOptions": {
    "tags": [ "croaring", "roaring", "bitmap" ],
    "licenseUrl": "http://opensource.org/licenses/MIT",
    "projectUrl": "https://github.com/RogueException/CRoaring",
    "repository": {
      "type": "git",
      "url": "git://github.com/RogueException/CRoaring"
    }
  },

  "buildOptions": {
    "debugType": "portable",
    "platform": "x64",
    "copyToOutput": {
      "include": [ "CRoaring.dll", "libCRoaring.so" ]
    }
  },

  "dependencies": {
    "NETStandard.Library": "1.6.0"
  },

  "frameworks": {
    "netstandard1.3": {}
  }
}
