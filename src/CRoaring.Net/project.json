{
  "version": "1.0.0-alpha1",
  "description": ".Net wrapper for CRoaring - a C implementation of Roaring Bitmaps",
  "authors": [ "RogueException" ],

  "packOptions": {
    "tags": [ "croaring", "roaring", "bitmap" ],
    "licenseUrl": "http://opensource.org/licenses/MIT",
    "projectUrl": "https://github.com/RogueException/CRoaring",
    "repository": {
      "type": "git",
      "url": "git://github.com/RogueException/CRoaring"
    },
    "files" : {
      "includeFiles" : [ "CRoaring.dll", "libCRoaring.so" ]
    }
  },

  "buildOptions": {
    "debugType": "portable",
    "platform": "x64",
    "copyToOutput": {
      "include": [ "CRoaring.dll", "libCRoaring.so" ]
    }
  },

  "dependencies": {
    "System.Collections.Concurrent": "4.0.12",
    "System.Runtime": "4.1.0",
    "System.Runtime.InteropServices": "4.1.0",
    "System.Threading": "4.0.11"
  },

  "frameworks": {
    "netstandard1.3": {}
  }
}
